{
  "name": "react-native-init",
  "version": "0.0.0",
  "description": "Bundle React Native CLI into a command line tool to create React Native apps",
  "license": "MIT",
  "bin": "index.js",
  "engines": {
    "node": ">=10.4"
  },
  "repository": "thachnn/react-native-init",
  "private": true,
  "workspaces": [
    "packages/cli",
    "packages/cli-doctor",
    "packages/cli-tools",
    "packages/cli-types"
  ],
  "scripts": {
    "build": "node ./scripts/build-webpack.js",
    "build:ts": "node ./node_modules/typescript/bin/tsc -b packages/cli-types packages/cli",
    "clean": "git clean -fdx build/ node_modules/.cache packages/",
    "postinstall": "git apply --reject --ignore-whitespace scripts/webpack-deps.patch"
  },
  "devDependencies": {
    "@babel/core": "thachnn/webpack-bundle#_babel/core-7.17.10",
    "@babel/preset-env": "thachnn/webpack-bundle#_babel/preset-env-7.17.10",
    "@babel/preset-typescript": "thachnn/webpack-bundle#_babel/preset-typescript-7.16.7",
    "babel-loader": "thachnn/webpack-bundle#babel-loader-8.2.5",
    "typescript": "^4.9.5",
    "webpack": "thachnn/webpack-bundle#v5.74.0"
  },
  "resolutions": {
    "@types/node": "^16.0.0"
  }
}
